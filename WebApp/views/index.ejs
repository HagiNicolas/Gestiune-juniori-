    <title>Farul Constanta - Juniori</title>
    <link rel="stylesheet" href="./style.css" />
  </head>
  <body>
    <div id="layout">
      <div class="header">
        <img class="logo" src="./logo-farul.svg" alt="logo Farul" />
      </div>
      <div class="main">
        <h1>Juniori Farul Constan»õa</h1>
        <div class="continut">
            <% rooms.forEach(function(room) { %>
              <div class="camere">
                <div class="camereUp">
                  <p class="numar"><b><%= room.NumarCamera %></b></p>
                  <img src="./pen2.svg" class="edit" id="edit<%=room.NumarCamera%>" height="24px" width="24px" />
                </div>
                <div class="jucatorii jucatorii-front">
                  <p class="jucator1"><%= room.players[0] %></p>
                  <br />
                  <p class="jucator2"><%= room.players[1] %></p>
                </div>
                <div class="jucatorii jucatorii-back">
                  <form method="post" action="/submit">
                    <input type="text" class="jucator1-back" name="juc1" placeholder="Nume jucator 1" value="" />
                    <input type="text" class="jucator2-back" name="juc2" placeholder="Nume jucator 2" value="" />
                    <br />
                    <button class="save" type="submit">Save</button>
                    <button class="cancel" type="button">Cancel</button>
                    <input type="hidden" name="camera" value="<%= room.NumarCamera %>"></input>
                  </form>
                </div>
              </div>
            <% }); %>
        
          
